{
  "name": "cryptan-desktop",
  "version": "0.0.0",
  "homepage": "https://github.com/acayseth/cryptan-desktop",
  "author": {
    "name": "acayseth",
    "email": "gorbulea.s@gmail.com"
  },
  "description": "Is a software application that is used to store and manage the passwords that a user has for various online accounts and security features. Password managers store the passwords in an encrypted format and provide secure access to all the password information with the help of a master password. ",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "start": "npm-run-all -l -p electron:tsc:watch ng:build:dev electron:run",
    "prestart": "rimraf dist build electron",
    "ng:build:dev": "ng build --delete-output-path=false --prod=false --base-href --watch",
    "ng:build:prod": "ng build --prod=true --base-href",
    "electron:tsc:watch": "tsc -w -p tsconfig.electron.json",
    "electron:run": "wait-on dist/index.html && electron . --develop --theme light"
  },
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@journeyapps/sqlcipher": "^5.2.0",
    "rxjs": "~6.6.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~10.2.4",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.31",
    "@types/validator": "^13.1.3",
    "codelyzer": "^6.0.0",
    "electron": "^12.0.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.1"
  },
  "license": "GPL-3.0",
